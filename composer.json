{
	"name": "symbiote/silverstripe-build",
	"description": "Phing build helpers for projects making use of the silverstripe-base installation process.",
	"type": "silverstripe-module",
	"keywords": ["silverstripe", "build"],
	"license": "BSD-3-Clause",
	"authors": [
	{
		"name": "Marcus Nyeholt",
		"email": "marcus@symbiote.com.au"
	}
	],
	"require": {
		"silverstripe/framework": "~3.1"
	},
	"extra": {
		"installer-name": "build",
		"branch-alias": {
			"dev-master": "3.1.x-dev"
		}
	},
	"replace": {
		"silverstripe-australia/build": "self.version"
	},
    "autoload": {
        "psr-0": {
            "Symbiote\\": "src/"
        }
    }
}
